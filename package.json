{
  "name": "zshy",
  "version": "0.0.1",
  "module": "./out/index.js",
  "type": "module",
  "private": true,
  "bin": {
    "zshy": "./out/index.js"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@biomejs/biome": "^2.0.5",
    "@types/node": "latest",
    "@types/react": "^19.1.8",
    "react": "^19.1.0",
    "tsx": "^4.20.3",
    "typescript": ">5.5.0"
  },
  "peerDependencies": {
    "typescript": ">5.5.0"
  },
  "dependencies": {
    "arg": "^5.0.2",
    "globby": "^14.1.0",
    "table": "^6.9.0"
  },
  "scripts": {
    "build": "tsx index.ts"
  },
  "zshy": {
    "exports": {
      ".": "./src/index.ts",
      "./utils": "./src/utils.ts",
      "./plugins/*": "./src/plugins/*"
    },
    "sourceDialects": [
      "@zod/source"
    ]
  },
  "main": "./out/index.cjs",
  "types": "./out/index.d.cts",
  "exports": {
    ".": {
      "@zod/source": "./src/index.ts",
      "types": "./out/index.d.cts",
      "import": "./out/index.js",
      "require": "./out/index.cjs"
    },
    "./utils": {
      "@zod/source": "./src/utils.ts",
      "types": "./out/utils.d.cts",
      "import": "./out/utils.js",
      "require": "./out/utils.cjs"
    },
    "./plugins/*": {
      "@zod/source": "./src/plugins/*",
      "import": "./out/src/plugins/*",
      "require": "./out/src/plugins/*"
    }
  },
  "files": [
    "out"
  ]
}
