{
  "name": "my-pkg",
  "version": "1.0.0",
  "description": "Test fixture for zshy - flat build (outDir: '.')",
  "type": "module",
  "scripts": {
    "build": "tsx ../../src/index.ts --project tsconfig.json"
  },
  "devDependencies": {
    "typescript": "^5.8.3"
  },
  "zshy": {
    "exports": {
      ".": "./src/index.ts"
    }
  },
  "bin": {
    "my-pkg": "./node_modules/my-pkg/dist/index.cjs"
  },
  "files": [
    "**/*.js",
    "**/*.mjs",
    "**/*.cjs",
    "**/*.d.ts",
    "**/*.d.mts",
    "**/*.d.cts"
  ],
  "main": "./index.cjs",
  "module": "./index.js",
  "types": "./index.d.cts",
  "exports": {
    ".": {
      "types": "./index.d.cts",
      "import": "./index.js",
      "require": "./index.cjs"
    }
  }
}
