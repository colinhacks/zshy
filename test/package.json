{
  "name": "zshy-test-fixture",
  "version": "1.0.0",
  "description": "Test fixture for zshy - represents a typical TypeScript library",
  "type": "module",
  "main": "./index.cjs",
  "types": "./index.d.cts",
  "bin": "./index.cjs",
  "scripts": {
    "build": "zshy"
  },
  "devDependencies": {
    "typescript": "^5.8.3"
  },
  "zshy": {
    "exports": {
      ".": "./src/index.ts",
      "./utils": "./src/utils.ts",
      "./plugins/*": "./src/plugins/*"
    },
    "bin": "./src/index.ts",
    "sourceDialects": [
      "@zod/source"
    ]
  },
  "files": [
    "dist"
  ],
  "module": "./index.js",
  "exports": {
    ".": {
      "@zod/source": "./src/index.ts",
      "types": "./index.d.cts",
      "import": "./index.js",
      "require": "./index.cjs"
    },
    "./utils": {
      "@zod/source": "./src/utils.ts",
      "types": "./utils.d.cts",
      "import": "./utils.js",
      "require": "./utils.cjs"
    },
    "./plugins/*": {
      "@zod/source": "./src/plugins/*",
      "types": "./plugins/*",
      "import": "./plugins/*",
      "require": "./plugins/*"
    }
  }
}
