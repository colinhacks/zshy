{
  "name": "my-pkg",
  "version": "1.0.0",
  "description": "Test fixture for zshy - represents a typical TypeScript library",
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.cts",
  "bin": "./dist/index.cjs",
  "scripts": {
    "build": "tsx ../src/index.ts --project tsconfig.basic.json"
  },
  "devDependencies": {
    "typescript": "^5.8.3"
  },
  "zshy": {
    "exports": {
      ".": "./src/index.ts",
      "./hello": "./src/hello.ts",
      "./utils": "./src/utils.ts",
      "./plugins": "./src/plugins/index.ts",
      "./plugins/*": "./src/plugins/*"
    },
    "bin": "./src/index.ts",
    "sourceDialects": [
      "@zod/source"
    ]
  },
  "files": [
    "dist"
  ],
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "@zod/source": "./src/index.ts",
      "types": "./dist/index.d.cts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./hello": {
      "@zod/source": "./src/hello.ts",
      "types": "./dist/hello.d.cts",
      "import": "./dist/hello.js",
      "require": "./dist/hello.cjs"
    },
    "./utils": {
      "@zod/source": "./src/utils.ts",
      "types": "./dist/utils.d.cts",
      "import": "./dist/utils.js",
      "require": "./dist/utils.cjs"
    },
    "./plugins": {
      "@zod/source": "./src/plugins/index.ts",
      "types": "./dist/plugins/index.d.cts",
      "import": "./dist/plugins/index.js",
      "require": "./dist/plugins/index.cjs"
    },
    "./plugins/*": {
      "@zod/source": "./src/plugins/*",
      "types": "./dist/plugins/*",
      "import": "./dist/plugins/*",
      "require": "./dist/plugins/*"
    }
  }
}
